{
	"name": "tomzx/finder",
	"description": "Improved, Symfony Finder compatible finder.",
	"type": "library",
	"keywords": [
		"finder"
	],
	"homepage": "https://github.com/tomzx/finder",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "https://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/finder/issues"
	},
	"require": {
		"php": ">=7.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"Finder\\": "src/Finder"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Finder\\Test\\": "tests/Finder"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2-dev"
		}
	}
}
